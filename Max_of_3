//Given three distinct integers A, B and C, print the second largest number among them.//

#include <bits/stdc++.h>
using namespace std;

int main() {
	int a,b,c;
	cin>>a>>b>>c;
	//int m = max(a,max(b,c));
	int m1 = max(a,b);
	int m2 = max(b,c);
	int m3 = max(a,c);
	if(m1<m2){
	    cout<<m1;
	}
	if(m2<m3){
	    cout<<m2;
	}
	if(m3<m1){
	    cout<<m3;
	}
	return 0;
}
