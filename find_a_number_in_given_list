***
You are given a list of N integers and a value K. Print 1 if K exists in the given list of N integers, otherwise print âˆ’1.
***


#include <bits/stdc++.h>
using namespace std;

int main() {
    int n,k,ans=0;cin>>n>>k;
    vector<int>v(n);
    for(int i=0;i<n;i++){
        cin>>v[i];
        if(v[i]==k) ans=1;
    }
    if(ans){
        cout<<1;
    }
    else{
        cout<<-1;
    }

	return 0;
}
