///////////////////////////////////////////////////////////
//	MAXIMUM SUBARRY SUM//
/////////////////////////////////
int main(int argc, char const *argv[])
{
	int n;cin>>n;
	//std::vector<vector<int> > v;
	for (int i = 0; i < n; i++)
	{
		cin>>a[i];
	}
	//priority_queue<int> q;
	int Msum =0,Csum=0;
	for(int i=0;i<n;i++){
		for(int j=i;j<n;j++){
			Csum=0;
			for(int k=i;k<=j;k++){
				Csum+=a[k];
			}
			if(Csum>Msum){
				Msum=Csum;
			}
		}
	}
	cout<<Msum<<endl;
	return 0;
}
