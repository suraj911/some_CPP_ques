#include<iostream>
#include<string>
using namespace std;

string op[1000];
int subsequences(string str,string op[]){
	if(str.length()==0){
		op[0]="";
		return 1;
	}

	int sizetillnow = subsequences(str.substr(1),op);

	for(int i=0;i<sizetillnow;i++){
		op[i+sizetillnow] = str[0] + op[i];
	}
	return 2*sizetillnow;
}



void printsubstring(string s)
{
	for(int i=0;i<s.length();i++){
		for(int j=i;j<s.length();j++){
			for(int k=i;k<=j;k++){
				cout<<s[k];
			}
			cout<<"\n";
		}
		//cout<<"\n";
	}
}


void printsubstring1(string s){
	int n = s.length();
	 for (int i = 0; i < n; i++)
        for (int len = 1; len <= n - i; len++)
            cout << s.substr(i, len) << endl;

}



int main(int argc, char const *argv[])
{
	string str;
	cin>>str;
	int size = subsequences(str,op);
	for (int i = 0; i < size; ++i)
	{
		cout<<op[i]<<"\n";
	}
	printsubstring(str);
	cout<<"=================="<<endl;
		printsubstring1(str);
	
	return 0;
}
