***
You're given the length of three sides a, b, and c respectively. Now check if these three sides can form a triangle or not
***


#include <iostream>
using namespace std;

int main() {
    int a,b,c;
    cin>>a>>b>>c;
    if(a+b<=c or b+c<=a or a+c<=b){
        cout<<"NO";
    }
    else{
        cout<<"YES";
    }
	return 0;
}
